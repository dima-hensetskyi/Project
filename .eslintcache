turn EditIcon;\r\n            case 'save':\r\n                return SaveIcon;\r\n            case 'cancel':\r\n                return CancelIcon;\r\n        }\r\n    }\r\n    return (\r\n        <div className={`icon-button-${size}`} onClick={onClick}>\r\n            <img src={getUrlByName(iconName)} alt={`${iconName} icon button`} />\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default Icon;\r\n","C:\\Users\\Іринка-смішинка\\Desktop\\Cursor Education\\Project\\src\\components\\currentBalance\\CurrentBalance.jsx",[],"C:\\Users\\Іринка-смішинка\\Desktop\\Cursor Education\\Project\\src\\components\\CurrentBalance\\CurrentBalance.jsx",[],"C:\\Users\\Іринка-смішинка\\Desktop\\Cursor Education\\Project\\src\\components\\Charges\\TableCharges.jsx",["74","75","76"],"C:\\Users\\Іринка-смішинка\\Desktop\\Cursor Education\\Project\\src\\components\\Charges\\Icon.jsx",["77"],"C:\\Users\\Іринка-смішинка\\Desktop\\Cursor Education\\Project\\src\\components\\Charges\\ChargeRow.jsx",["78"],"C:\\Users\\Іринка-смішинка\\Desktop\\Cursor Education\\Project\\src\\components\\Charts\\Charts.js",[],"C:\\Users\\Іринка-смішинка\\Desktop\\Cursor Education\\Project\\src\\components\\Categories\\Categories.js",[],"C:\\Users\\Іринка-смішинка\\Desktop\\Cursor Education\\Project\\src\\components\\Tables\\Tables.js",[],"C:\\Users\\Іринка-смішинка\\Desktop\\Cursor Education\\Project\\src\\components\\Incomes\\TableIncomes.js",[],{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","severity":1,"message":"84","line":6,"column":5,"nodeType":"85","messageId":"86","endLine":6,"endColumn":9},{"ruleId":"83","severity":1,"message":"87","line":5,"column":8,"nodeType":"85","messageId":"86","endLine":5,"endColumn":25},{"ruleId":"83","severity":1,"message":"88","line":5,"column":41,"nodeType":"85","messageId":"86","endLine":5,"endColumn":50},{"ruleId":"83","severity":1,"message":"89","line":12,"column":10,"nodeType":"85","messageId":"86","endLine":12,"endColumn":19},{"ruleId"